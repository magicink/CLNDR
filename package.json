{
  "name": "@brandontom/luxon-clndr",
  "version": "0.0.1",
  "description": "A jQuery calendar plugin that uses HTML templates.",
  "keywords": [
    "clndr",
    "calendar",
    "jquery",
    "plugin",
    "widget",
    "jquery-plugin"
  ],
  "homepage": "https://github.com/kylestetz/CLNDR",
  "bugs": {
    "url": "https://github.com/kylestetz/CLNDR/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kylestetz/CLNDR"
  },
  "license": "MIT",
  "author": "Kyle Stetz",
  "contributors": [
    "Mike Gioia <mike@teachboost.com>"
  ],
  "exports": {
    ".": {
      "types": "./types/clndr.d.ts",
      "import": "./src/clndr.js",
      "require": "./src/clndr.js"
    },
    "./facade": {
      "types": "./types/clndr.d.ts",
      "import": "./dist/src/ts/index.js"
    },
    "./legacy": {
      "import": "./src/clndr.js",
      "require": "./src/clndr.js"
    }
  },
  "main": "src/clndr.js",
  "types": "types/clndr.d.ts",
  "files": [
    "src/clndr.js",
    "clndr.min.js",
    "types",
    "dist/src/ts",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "baseline:snapshots": "bun scripts/capture-baseline.mjs",
    "build:ts": "tsc -p tsconfig.json",
    "ci:check": "npm run type-check && npm run build:ts && npm run test -- --coverage && npm run smoke",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky || true",
    "smoke": "node scripts/smoke.mjs",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "prettier": "@brandontom/prettier",
  "dependencies": {
    "jquery": ">=3.7",
    "moment": ">=2.30.1"
  },
  "devDependencies": {
    "@brandontom/prettier": "^2.0.2",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/jest": "^29.5.14",
    "grunt": "*",
    "grunt-contrib-less": "^3.0.0",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-contrib-watch": "^1.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "puppeteer": "^24.29.1",
    "sort-package-json": "^3.4.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
